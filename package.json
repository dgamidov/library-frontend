{
  "name": "library-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-dev": "vue-cli-service build --mode=development",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy-test": "aws --endpoint-url=https://storage.yandexcloud.net s3 cp --recursive --exclude .DS_Store dist/ s3://library-test/",
    "deploy": "aws --endpoint-url=https://storage.yandexcloud.net s3 cp --recursive --exclude .DS_Store dist/ s3://library/"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "core-js": "^3.8.3",
    "js-yaml": "^4.1.0",
    "roboto-fontface": "*",
    "vue": "^3.2.13",
    "vue-router": "^4.1.3",
    "vuetify": "^3.0.0-beta.7",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-cli-plugin-vuetify": "~2.5.1",
    "webpack-plugin-vuetify": "^2.0.0-alpha.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
